class Solution {
    public void moveZeroes(int[] nums) {
        int n=nums.length;
        int in=0;
        int temp[]=new int[n];
        for(int i=0;i<n;i++)
        {
            if(nums[i]!=0)
            {
                temp[in++]=nums[i];
            }
        }
        for(int i=0;i<temp.length;i++)
        {
            nums[i]=temp[i];
        }
        return;
        
    }
}
