
import java.util.*;
class Main {
    public static void main(String[] args) {
        int arr[]={6,5,8,9,1,3};
        int sma=arr[0];
        int sec=Integer.MAX_VALUE ;
        for(int i=0;i<arr.length;i++)
        {
            if(arr[i]<sma)
            {
                sec=sma;
                sma=arr[i];
            }
            if(arr[i]>sma && arr[i]<sec)
            {
                sec=arr[i];
            }
        }
        System.out.println(sec);
    }
}
